# Server
PORT=8081
SERVER_PORT=8081
GIN_MODE=release
ENVIRONMENT=development

# Database (not used by Map Service, but required by pkg/config validation)
DB_NAME=map_db_placeholder

# Google Maps API
GOOGLE_MAPS_API_KEY=your_api_key_here

# Redis
REDIS_HOST=localhost
REDIS_PORT=6380
REDIS_DB=5
REDIS_PASSWORD=

# Cache
CACHE_PLACE_DETAILS_TTL=3600

# Rate Limiting
RATE_LIMIT_PER_MINUTE=10

# Logging
LOG_LEVEL=info
LOG_FORMAT=json
