{
    "dashboard": {
        "title": "Tabelogo K6 Load Testing Dashboard",
        "tags": [
            "k6",
            "load-testing",
            "performance"
        ],
        "timezone": "browser",
        "schemaVersion": 16,
        "version": 0,
        "refresh": "5s",
        "panels": [
            {
                "id": 1,
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 0
                },
                "type": "graph",
                "title": "HTTP Request Rate (RPS)",
                "targets": [
                    {
                        "expr": "rate(http_requests_total[1m])",
                        "legendFormat": "{{job}} - {{method}} {{path}}",
                        "refId": "A"
                    }
                ],
                "yaxes": [
                    {
                        "format": "reqps",
                        "label": "Requests/sec"
                    },
                    {
                        "format": "short"
                    }
                ]
            },
            {
                "id": 2,
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 0
                },
                "type": "graph",
                "title": "Response Time (P95, P99)",
                "targets": [
                    {
                        "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
                        "legendFormat": "P95",
                        "refId": "A"
                    },
                    {
                        "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
                        "legendFormat": "P99",
                        "refId": "B"
                    }
                ],
                "yaxes": [
                    {
                        "format": "s",
                        "label": "Duration"
                    },
                    {
                        "format": "short"
                    }
                ]
            },
            {
                "id": 3,
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 8
                },
                "type": "graph",
                "title": "Error Rate",
                "targets": [
                    {
                        "expr": "rate(http_requests_total{status=~\"4..\"}[1m])",
                        "legendFormat": "4xx - {{job}}",
                        "refId": "A"
                    },
                    {
                        "expr": "rate(http_requests_total{status=~\"5..\"}[1m])",
                        "legendFormat": "5xx - {{job}}",
                        "refId": "B"
                    }
                ],
                "yaxes": [
                    {
                        "format": "reqps",
                        "label": "Errors/sec"
                    },
                    {
                        "format": "short"
                    }
                ]
            },
            {
                "id": 4,
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 8
                },
                "type": "graph",
                "title": "Cache Hit Rate",
                "targets": [
                    {
                        "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m])) * 100",
                        "legendFormat": "Hit Rate %",
                        "refId": "A"
                    }
                ],
                "yaxes": [
                    {
                        "format": "percent",
                        "label": "Hit Rate",
                        "max": 100,
                        "min": 0
                    },
                    {
                        "format": "short"
                    }
                ]
            },
            {
                "id": 5,
                "gridPos": {
                    "h": 8,
                    "w": 8,
                    "x": 0,
                    "y": 16
                },
                "type": "stat",
                "title": "Total Requests",
                "targets": [
                    {
                        "expr": "sum(http_requests_total)",
                        "refId": "A"
                    }
                ],
                "options": {
                    "graphMode": "area",
                    "colorMode": "value"
                }
            },
            {
                "id": 6,
                "gridPos": {
                    "h": 8,
                    "w": 8,
                    "x": 8,
                    "y": 16
                },
                "type": "stat",
                "title": "Google API Calls (Should be 0)",
                "targets": [
                    {
                        "expr": "sum(google_api_calls_total)",
                        "refId": "A"
                    }
                ],
                "options": {
                    "graphMode": "area",
                    "colorMode": "value"
                },
                "fieldConfig": {
                    "defaults": {
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "value": 0,
                                    "color": "green"
                                },
                                {
                                    "value": 1,
                                    "color": "red"
                                }
                            ]
                        }
                    }
                }
            },
            {
                "id": 7,
                "gridPos": {
                    "h": 8,
                    "w": 8,
                    "x": 16,
                    "y": 16
                },
                "type": "stat",
                "title": "Active Goroutines",
                "targets": [
                    {
                        "expr": "sum(go_goroutines)",
                        "refId": "A"
                    }
                ],
                "options": {
                    "graphMode": "area",
                    "colorMode": "value"
                }
            },
            {
                "id": 8,
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 24
                },
                "type": "graph",
                "title": "Memory Usage",
                "targets": [
                    {
                        "expr": "go_memstats_alloc_bytes",
                        "legendFormat": "{{job}}",
                        "refId": "A"
                    }
                ],
                "yaxes": [
                    {
                        "format": "bytes",
                        "label": "Memory"
                    },
                    {
                        "format": "short"
                    }
                ]
            },
            {
                "id": 9,
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 24
                },
                "type": "graph",
                "title": "GC Pause Duration",
                "targets": [
                    {
                        "expr": "rate(go_gc_duration_seconds_sum[5m])",
                        "legendFormat": "{{job}}",
                        "refId": "A"
                    }
                ],
                "yaxes": [
                    {
                        "format": "s",
                        "label": "Duration"
                    },
                    {
                        "format": "short"
                    }
                ]
            }
        ]
    }
}