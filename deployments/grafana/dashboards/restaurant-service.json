{
    "dashboard": {
        "title": "Restaurant Service Metrics",
        "tags": [
            "restaurant",
            "microservices"
        ],
        "timezone": "browser",
        "schemaVersion": 16,
        "version": 0,
        "refresh": "5s",
        "panels": [
            {
                "id": 1,
                "title": "Cache Hit Rate",
                "type": "graph",
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 0
                },
                "targets": [
                    {
                        "expr": "rate(restaurant_cache_hits_total[5m]) / (rate(restaurant_cache_hits_total[5m]) + rate(restaurant_cache_misses_total[5m])) * 100",
                        "legendFormat": "Cache Hit Rate %",
                        "refId": "A"
                    }
                ],
                "yaxes": [
                    {
                        "format": "percent",
                        "min": 0,
                        "max": 100
                    },
                    {
                        "format": "short"
                    }
                ]
            },
            {
                "id": 2,
                "title": "Map Service API Calls",
                "type": "graph",
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 0
                },
                "targets": [
                    {
                        "expr": "rate(restaurant_map_service_calls_total{status=\"success\"}[5m])",
                        "legendFormat": "Success",
                        "refId": "A"
                    },
                    {
                        "expr": "rate(restaurant_map_service_calls_total{status=\"error\"}[5m])",
                        "legendFormat": "Error",
                        "refId": "B"
                    }
                ]
            },
            {
                "id": 3,
                "title": "Response Time (p95)",
                "type": "graph",
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 8
                },
                "targets": [
                    {
                        "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"restaurant-service\"}[5m]))",
                        "legendFormat": "p95 Response Time",
                        "refId": "A"
                    },
                    {
                        "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"restaurant-service\"}[5m]))",
                        "legendFormat": "p50 Response Time",
                        "refId": "B"
                    }
                ],
                "yaxes": [
                    {
                        "format": "s"
                    },
                    {
                        "format": "short"
                    }
                ]
            },
            {
                "id": 4,
                "title": "Request Rate",
                "type": "graph",
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 8
                },
                "targets": [
                    {
                        "expr": "rate(http_requests_total{job=\"restaurant-service\"}[5m])",
                        "legendFormat": "{{method}} {{path}}",
                        "refId": "A"
                    }
                ]
            },
            {
                "id": 5,
                "title": "Stale Data Returns",
                "type": "stat",
                "gridPos": {
                    "h": 4,
                    "w": 6,
                    "x": 0,
                    "y": 16
                },
                "targets": [
                    {
                        "expr": "restaurant_stale_data_returns_total",
                        "refId": "A"
                    }
                ]
            },
            {
                "id": 6,
                "title": "Total Restaurants Cached",
                "type": "stat",
                "gridPos": {
                    "h": 4,
                    "w": 6,
                    "x": 6,
                    "y": 16
                },
                "targets": [
                    {
                        "expr": "restaurant_cache_hits_total + restaurant_cache_misses_total",
                        "refId": "A"
                    }
                ]
            },
            {
                "id": 7,
                "title": "Sync Duration (avg)",
                "type": "stat",
                "gridPos": {
                    "h": 4,
                    "w": 6,
                    "x": 12,
                    "y": 16
                },
                "targets": [
                    {
                        "expr": "rate(restaurant_sync_duration_seconds_sum[5m]) / rate(restaurant_sync_duration_seconds_count[5m])",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "s"
                    }
                }
            },
            {
                "id": 8,
                "title": "Error Rate",
                "type": "stat",
                "gridPos": {
                    "h": 4,
                    "w": 6,
                    "x": 18,
                    "y": 16
                },
                "targets": [
                    {
                        "expr": "rate(http_requests_total{job=\"restaurant-service\",status=~\"5..\"}[5m]) / rate(http_requests_total{job=\"restaurant-service\"}[5m]) * 100",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "percent",
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "value": 0,
                                    "color": "green"
                                },
                                {
                                    "value": 1,
                                    "color": "yellow"
                                },
                                {
                                    "value": 5,
                                    "color": "red"
                                }
                            ]
                        }
                    }
                }
            }
        ]
    },
    "overwrite": true
}